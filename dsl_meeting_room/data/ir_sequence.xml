<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="dsl_meeting_facility_code_sequence">
            <field name="name">Meeting Room Facility Code</field>
            <field name="code">dsl.meeting.facility</field>
            <field name="implementation">standard</field>
            <field name="active">TRUE</field>
            <field name="prefix">MF-</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">0</field>
        </record>


        <record model="ir.sequence" id="dsl_booking_code_sequence">
            <field name="name">Booking Room Code</field>
            <field name="code">dsl.booking.room</field>
            <field name="implementation">standard</field>
            <field name="active">TRUE</field>
            <field name="prefix">BR-</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">0</field>
        </record>
    </data>
</odoo>